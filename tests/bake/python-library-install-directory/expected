!bake
python --version
python -m py_compile test.py
(exit SUCCESS)
!bake install
python --version
mkdir -p /usr/lib/custom-directory
cp test.pyc /usr/lib/custom-directory/test.pyc
(exit SUCCESS)
!bake uninstall
python --version
rm -f /usr/lib/custom-directory/test.pyc
(exit SUCCESS)
!bake clean
python --version
rm -f test.pyc
(exit SUCCESS)
